{
  "version": 2,
  "builds": [
    {
      "src": "newServer/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "newServer/index.js"
    }
  ]
}
